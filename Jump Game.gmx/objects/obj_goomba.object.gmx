                vspd = -4;
            }
            vspd = -1*abs(vspd);
    }
        
    instance_destroy();
    }
    else {
        with (inst) {
            x = xstart;
            y = ystart;
            //instance_destroy();
            
        }
    }
}

if(place_meeting(x,y+1, obj_enemy)) {
    vspd = 0;
}


if (place_meeting(x + hspd,y,obj_enemy)) {
    hspd = hspd * -1;
}

//shimmy
var a_inst;
inst = instance_place(x,y,obj_enemy);
if (inst != noone) {
    move_away(inst,1);
}

//shimmy
var b_inst;
inst = instance_place(x,y,obj_floor);
if (inst != noone) {
